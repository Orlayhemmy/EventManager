{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --watch server --exec babel-node -- server/app.js",
    "client:test": "jest client/ && codecov",
    "test": "npm run unmigrate && npm run migrate && npm run test:run && npm run client:test",
    "test:run": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --require babel-register --require babel-polyfill --recursive server/test/index.js --timeout 30000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "client-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "unmigrate": "NODE_ENV=test sequelize db:migrate:undo:all",
    "migrate": "NODE_ENV=test sequelize db:migrate && npm run seed",
    "seed": "NODE_ENV=test sequelize db:seed:all",
    "unseed": "NODE_ENV=test sequelize db:seed:undo:all",
    "build": "webpack -d && webpack-dev-server --content-base client/ --inline --hot --port 3000 --history-api-fallback --silent"
  },
  "author": "Olayemi Lawal",
  "license": "ISC",
  "devDependencies": {
